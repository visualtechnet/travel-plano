{
	"env": {
		"browser": true,
		"commonjs": true,
		"node": true,
		"jquery": true,
		"jest": true
	},
	"extends": [
		"airbnb-base"
	],
	"plugins": ["jest"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"no-console": "off",
		"max-len": "off",
		"no-return-await": "off"
	}
}